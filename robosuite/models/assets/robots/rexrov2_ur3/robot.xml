            <!-- rov 6 dof joint -->
            <joint name="wrist_x_slide_joint" damping="2000" type="slide" pos="0 0 0" axis="1 0 0" limited="true" range="-1.0 1.0" />
            <joint name="wrist_y_slide_joint" damping="2000" type="slide" pos="0 0 0" axis="0 1 0" limited="true" range="-1.0 1.0" />
            <joint name="wrist_z_slide_joint" damping="5000" type="slide" pos="0 0 0" axis="0 0 1" limited="true" range="-1.0 1.0" />
            <joint name="wrist_z_hinge_joint" damping="5000" type="hinge" pos="0 0 0" axis="0 0 1" limited="true" range="-1. 1." />
            <joint name="wrist_y_hinge_joint" damping="5000" type="hinge" pos="0 0 0" axis="0 1 0" limited="true" range="-1. 1." />
            <joint name="wrist_x_hinge_joint" damping="5000" type="hinge" pos="0 0 0" axis="1 0 0" limited="true" range="-1. 1." />
